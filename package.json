{
  "name": "@fuzzysaj/ip-to-geo-location",
  "version": "1.2.2",
  "scripts": {
    "prepare": "tsc",
    "cmd-line-lookup": "DEBUG=ip-to-geo-location ts-node -r dotenv/config src/cmd-line-lookup.ts",
    "pretest": "tsc -p .",
    "test": "nyc mocha",
    "quick-test": "DEBUG=* ts-node -r dotenv/config test/misc/quick-test.ts",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  },
  "license": "GPL-3.0",
  "repository": "fuzzysaj/ip-to-geo-location",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "fuzzysaj",
  "dependencies": {
    "@fuzzysaj/location-to-usa-county": "^1.0.4",
    "@fuzzysaj/nat-earth-geojson-cache": "^1.0.4",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "geojson-geometries-lookup": "^0.3.1",
    "maxmind": "^4.1.1",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.27.0",
    "tar-stream": "^2.1.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@turf/helpers": "^6.1.4",
    "@types/chai": "^4.2.11",
    "@types/chai-almost": "^1.0.1",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.4",
    "@types/ramda": "^0.27.4",
    "@types/tar-stream": "^2.1.0",
    "chai-almost": "^1.0.1",
    "codecov": "^3.6.5",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
