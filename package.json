{
  "name": "@fuzzysaj/ip-to-geo-location",
  "version": "1.0.3",
  "scripts": {
    "prepare": "tsc",
    "cmd-line-lookup": "DEBUG=ip-to-geo-location ts-node src/cmd-line-lookup.ts",
    "pretest": "tsc -p .",
    "test": "DEBUG=ip-to-geo-location nyc mocha",
    "quick-test": "DEBUG=* ts-node test/misc/quick-test.ts",
    "posttest": "nyc report --reporter=json"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": false
  },
  "license": "GPL-3.0",
  "repository": "fuzzysaj/ip-to-geo-location",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "fuzzysaj",
  "dependencies": {
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "debug": "^4.1.1",
    "geojson-geometries-lookup": "^0.3.0",
    "maxmind": "^3.0.5",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "shp2json": "^1.2.1",
    "tar-stream": "^2.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@turf/helpers": "^6.1.4",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.3",
    "@types/tar-stream": "^1.6.1",
    "codecov": "^3.5.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
